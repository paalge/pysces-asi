#!/usr/bin/python

# Script to run graphical version of pysces_asi

from pysces_asi import GUI
import wx
import logging

logging.basicConfig(
    filename='/home/aurora/pysces_asi.log', format='%(asctime)s - %(name)s -  %(levelname)s - %(message)s', level=logging.DEBUG)

# setup app
app = wx.PySimpleApp()

frame = GUI.MainFrame()
frame.Show()

app.MainLoop()
